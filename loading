    AssetStatusLabel.TextXAlignment = Enum.TextXAlignment.Left
    AssetStatusLabel.Parent = AssetFrame
    
    -- İçerik frame'i (animasyon için)
    local ContentFrame = Instance.new("Frame")
    ContentFrame.Name = "ContentFrame"
    ContentFrame.Size = UDim2.new(1, -40, 0, 80)
    ContentFrame.Position = UDim2.new(0, 20, 0, 115)
    ContentFrame.BackgroundTransparency = 1
    ContentFrame.ClipsDescendants = true
    ContentFrame.Parent = MainFrame
    
    -- Loading text
    local LoadingText = Instance.new("TextLabel")
    LoadingText.Name = "LoadingText"
    LoadingText.Size = UDim2.new(1, 0, 0, 20)
    LoadingText.Position = UDim2.new(0, 0, 0, 10)
    LoadingText.BackgroundTransparency = 1
    LoadingText.Text = "Initializing core components..."
    LoadingText.TextColor3 = Color3.new(1, 1, 1)
    LoadingText.Font = Enum.Font.Gotham
    LoadingText.TextSize = 16
    LoadingText.TextXAlignment = Enum.TextXAlignment.Left
    LoadingText.Parent = ContentFrame
    
    -- Progress bar
    local ProgressBar = Instance.new("Frame")
    ProgressBar.Name = "ProgressBar"
    ProgressBar.Size = UDim2.new(1, 0, 0, 6)
    ProgressBar.Position = UDim2.new(0, 0, 0, 40)
    ProgressBar.BackgroundColor3 = Color3.new(0.2, 0.2, 0.2)
    ProgressBar.BorderSizePixel = 0
    ProgressBar.Parent = ContentFrame
    
    local ProgressFill = Instance.new("Frame")
    ProgressFill.Name = "ProgressFill"
    ProgressFill.Size = UDim2.new(0, 0, 1, 0)
    ProgressFill.BackgroundColor3 = Color3.new(0.2, 0.6, 1)
    ProgressFill.BorderSizePixel = 0
    ProgressFill.Parent = ProgressBar
    
    local ProgressCorner = Instance.new("UICorner")
    ProgressCorner.CornerRadius = UDim.new(0, 3)
    ProgressCorner.Parent = ProgressBar
    
    -- Timer (ortalanmış)
    local Timer = Instance.new("TextLabel")
    Timer.Name = "Timer"
    Timer.Size = UDim2.new(1, 0, 0, 20)
    Timer.Position = UDim2.new(0, 0, 0, 60)
    Timer.BackgroundTransparency = 1
    Timer.Text = "Estimated time: 05:00"
    Timer.TextColor3 = Color3.new(1, 1, 1)
    Timer.Font = Enum.Font.Gotham
    Timer.TextSize = 14
    Timer.TextXAlignment = Enum.TextXAlignment.Center
    Timer.Parent = ContentFrame
    
    -- Taşıma işlevselliği
    local UserInputService = game:GetService("UserInputService")
    
    local dragging
    local dragInput
    local dragStart
    local startPos
    
    local function update(input)
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    
    TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)
    
    -- Buton işlevleri
    CloseButton.MouseButton1Click:Connect(function()
        local tween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
            Size = UDim2.new(0, 0, 0, 0),
            BackgroundTransparency = 1
        })
        tween:Play()
        tween.Completed:Wait()
        ScreenGui:Destroy()
    end)
    
    local minimized = false
    ToggleButton.MouseButton1Click:Connect(function()
        minimized = not minimized

        if minimized then
            ToggleButton.Text = "+"
            
            -- Tüm içeriği gizle, sadece başlık çubuğunu göster
            UserFrame.Visible = false
            ContentFrame.Visible = false
            AssetFrame.Visible = false
            TopDivider.Visible = false
            BottomDivider.Visible = false
            
            -- Ana frame boyutunu küçült
            local frameTween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 500, 0, 30)
            })
            frameTween:Play()
        else
            ToggleButton.Text = "−"
            
            -- Tüm içeriği göster
            UserFrame.Visible = true
            ContentFrame.Visible = true
            AssetFrame.Visible = true
            TopDivider.Visible = true
            BottomDivider.Visible = true
            
            -- Ana frame boyutunu büyüt
            local frameTween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 500, 0, 250)
            })
            frameTween:Play()
        end
    end)
        -- YENİ SÜRÜKLEME SİSTEMİ
    local dragging
    local dragInput
    local dragStart
    local startPos

    local function update(input)
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end

    -- MainFrame için sürükleme başlangıcı
    MainFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = MainFrame.Position
            
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)

    -- Butonlar için sürükleme engelleme
    local function stopPropagation(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            input.StopPropagation()
        end
    end

    CloseButton.InputBegan:Connect(stopPropagation)
    ToggleButton.InputBegan:Connect(stopPropagation)

    -- Sürükleme işlemi
    MainFrame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)

    -- Buton işlevleri (önceki kodunuzdaki gibi devam ediyor)
    CloseButton.MouseButton1Click:Connect(function()
        local tween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
            Size = UDim2.new(0, 0, 0, 0),
            BackgroundTransparency = 1
        })
        tween:Play()
        tween.Completed:Wait()
        ScreenGui:Destroy()
    end)

    local minimized = false
    ToggleButton.MouseButton1Click:Connect(function()
        minimized = not minimized

        if minimized then
            ToggleButton.Text = "+"
            UserFrame.Visible = false
            ContentFrame.Visible = false
            AssetFrame.Visible = false
            TopDivider.Visible = false
            BottomDivider.Visible = false
            
            local frameTween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 500, 0, 30)
            })
            frameTween:Play()
        else
            ToggleButton.Text = "−"
            UserFrame.Visible = true
            ContentFrame.Visible = true
            AssetFrame.Visible = true
            TopDivider.Visible = true
            BottomDivider.Visible = true
            
            local frameTween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.InOut), {
                Size = UDim2.new(0, 500, 0, 250)
            })
            frameTween:Play()
        end
    end)

    -- Açılış animasyonu
    MainFrame.Size = UDim2.new(0, 0, 0, 0)
    local openTween = TweenService:Create(MainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 500, 0, 250)
    })
    openTween:Play()
    
    -- Loading animasyonu
    local startTime = tick()
    local duration = 300 -- 5 dakika (300 saniye)
    
    local loadingTexts = {
        "Initializing core components...",
        "Loading asset bundles (12/47)...",
        "Compiling shaders... 28% complete",
        "Verifying data integrity...",
        "Establishing secure connection...",
        "Allocating memory resources...",
        "Loading textures... 256/1024",
        "Optimizing scene graph...",
        "Validating user permissions...",
        "Finalizing initialization sequence..."
    }
    
    local function formatTime(seconds)
        local mins = math.floor(seconds / 60)
        local secs = math.floor(seconds % 60)
        return string.format("%02d:%02d", mins, secs)
    end
    
    local connection
    connection = game:GetService("RunService").Heartbeat:Connect(function()
        local elapsed = tick() - startTime
        local remaining = math.max(0, duration - elapsed)
        
        if not minimized then
            -- Progress bar
            local progress = math.min(1, elapsed / duration)
            ProgressFill.Size = UDim2.new(progress, 0, 1, 0)
            
            -- Timer text
            Timer.Text = "Estimated time: " .. formatTime(remaining)
            
            -- Loading text değişimi
            local textIndex = math.floor((elapsed * 2) % #loadingTexts) + 1
            LoadingText.Text = loadingTexts[textIndex]
            
            -- Asset durum güncelleme
            AssetStatusLabel.Text = "Status: "..math.floor(progress*100).."% loaded"
        end
        
        if elapsed >= duration then
            connection:Disconnect()
            
            -- Kapanış animasyonu
            local closeTween = TweenService:Create(MainFrame, TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
                Size = UDim2.new(0, 0, 0, 0),
                BackgroundTransparency = 1
            })
            closeTween:Play()
            closeTween.Completed:Wait()
            ScreenGui:Destroy()
        end
    end)
    
    return ScreenGui
end

-- GUI'yi oyuncuya ekle
local FakeLoadingGui = CreateFakeLoadingGui()
FakeLoadingGui.Parent = Player:WaitForChild("PlayerGui")
