-- Fake Loading GUI Script
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local GuiService = game:GetService("GuiService")
local TweenService = game:GetService("TweenService")

-- Executor tespit fonksiyonu
local function GetExecutorInfo()
    -- Bilinen executor işaretleri
    local executorSignatures = {
        ["Synapse"] = {check = function() return syn and true or false end, version = function() return tostring(syn and syn.version or "Unknown") end},
        ["ScriptWare"] = {check = function() return SW_LOADED and true or false end},
        ["Krnl"] = {check = function() return krnl and true or false end, version = function() return tostring(krnl and krnl.version or "Unknown") end},
        ["Fluxus"] = {check = function() return fluxus and true or false end, version = function() return tostring(fluxus and fluxus.version or "Unknown") end},
        ["Comet"] = {check = function() return comet and true or false end},
        ["Electron"] = {check = function() return getexecutorname and true or false end, name = function() return tostring(getexecutorname and getexecutorname() or "Unknown") end},
        ["JJSploit"] = {check = function() return jjsploit and true or false end}
    }

    for name, data in pairs(executorSignatures) do
        if data.check() then
            local version = data.version and data.version() or "Unknown"
            local executorName = data.name and data.name() or name
            return executorName..(version ~= "Unknown" and " v"..version or "")
        end
    end

    -- Genel kontrol
    if getexecutorname then
        return tostring(getexecutorname() or "Unknown Executor")
    end

    return "Unknown Executor"
end

